<!--miniprogram/pages/myPage/myComments/myComments.wxml-->
<view class="myPage">
  <button style="background-color:#fed32e;width:100%;height:300rpx;position:relative;">
      <view class="myRate">{{myRate}}</view>
      <image class="myComment-avatar" src="{{avatarUrl}}" mode="scaleToFill"></image>
    <view class="myPage-nickname">{{nickName}}</view>
  </button>
</view>
<view class="myComments" style="display: block; box-sizing: border-box; height: 70rpx">所有评价</view>
<view class="frame comment">
  <view class="firstline">
    <view class="comment-serviceType">服务类别：</view>
    <view class="comment-number">订单号：</view>
  </view>
  <view class="comment-serviceTime">服务时间：</view>
  <view class="lastline">
    <view class="comment-fee">实付款：</view>
    <view class="comment"></view>
  </view>
</view>